{
  "cSpell.words": [
    "AsyncRAT",
    "bashlite",
    "BlackNET",
    "Censys",
    "darkcomet",
    "DarkGate",
    "DcRat",
    "dotenv",
    "Dridex",
    "ERMAC",
    "iocs",
    "jarm",
    "malpedia",
    "NimPlant",
    "PoshC2",
    "pydantic",
    "RisePro",
    "shadowpad",
    "spynote",
    "Supershell",
    "tarpit",
    "tarpits",
    "thehappydinoa",
    "Threatfox"
  ],
  "editor.defaultFormatter": "ms-python.black-formatter",
  "isort.check": true,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
      "source.organizeImports": "explicit"
    }
  },
  "isort.args": [
    "--profile",
    "black"
  ],
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[dockercompose]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
